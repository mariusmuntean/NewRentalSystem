<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  	<servlet>
		<servlet-name>rentalServlet</servlet-name>
		<servlet-class>de.tum.os.drs.server.ServiceImpl</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>rentalServlet</servlet-name>
		<url-pattern>/newrentalsystem/rentalService</url-pattern>
	</servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>NewRentalSystem.html</welcome-file>
  </welcome-file-list>

</web-app>
